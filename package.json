{
    "private": true,
    "scripts": {
        "dev": "nuxt -c client/nuxt.config.js",
        "build": "nuxt build -c client/nuxt.config.js",
        "start": "APP_PORT=2999 APP_HOST=localhost nuxt start -c client/nuxt.config.js",
        "generate": "APP_PORT=2999 APP_HOST=localhost nuxt generate -c client/nuxt.config.js",
        "analize": "APP_PORT=2999 APP_HOST=localhost nuxt generate -c client/nuxt.config.js --analyze",
        "prod": "APP_PORT=2999 APP_HOST=localhost nuxt generate -c client/nuxt.config.js && APP_PORT=2999 APP_HOST=localhost nuxt start -c client/nuxt.config.js",
        "lint": "vue-cli-service lint --fix client/"
    },
    "dependencies": {
        "@nuxtjs/router": "^1.5.0",
        "axios": "^0.19.0",
        "dotenv": "^8.1.0",
        "js-cookie": "^2.2.1",
        "lodash": "^4.17.15",
        "nuxt": "^2.9.2",
        "qs": "^6.8.0",
        "vform": "^1.0.1",
        "vue-i18n": "^8.14.0",
        "vue-router": "^3.1.3",
        "vuejs-paginate": "^2.1.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-eslint": "^3.11.0",
        "@vue/cli-service": "^3.11.0",
        "@vue/eslint-config-standard": "^4.0.0",
        "acorn": "^7.0.0",
        "node-sass": "^4.12.0",
        "sass-loader": "^8.0.0",
        "webpack": "^4.39.3",
        "webpack-cli": "3.3.8"
    }
}
